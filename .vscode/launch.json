{
    "configurations": [
        {
            "type": "java",
            "name": "ErrorResponse",
            "request": "launch",
            "mainClass": "com.mevy.gamesapi.resources.exceptions.ErrorResponse",
            "projectName": "gamesapi"
        },
        {
            "type": "java",
            "name": "GlobalExceptionHandler",
            "request": "launch",
            "mainClass": "com.mevy.gamesapi.resources.exceptions.GlobalExceptionHandler",
            "projectName": "gamesapi"
        },
        {
            "type": "java",
            "name": "Spring Boot-GamesapiApplication<gamesapi>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.mevy.gamesapi.GamesapiApplication",
            "projectName": "gamesapi",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=55227 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=gamesapi"
        }
    ]
}